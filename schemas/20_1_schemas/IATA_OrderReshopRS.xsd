		<xs:sequence>
			<xs:element name="FareGroup" type="FareGroupType" minOccurs="1" maxOccurs="unbounded"/>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="FarePriceTypeType">
		<xs:annotation>
			<xs:documentation>The type of fair price applied to a Fare Detail (e.g. filed amount, net amount, or a sell amount).</xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="FarePriceTypeCode" type="CodeType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
					<xs:documentation>Indicates if the fare price provided is a filed amount, net amount, or a sell amount.</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="Price" type="PriceType" minOccurs="1" maxOccurs="1">
				<xs:annotation>
