				<xs:element name="Request" type="RequestType" minOccurs="1" maxOccurs="1">
					<xs:annotation>
						<xs:documentation>Order Change request information.</xs:documentation>
					</xs:annotation>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
		
		
